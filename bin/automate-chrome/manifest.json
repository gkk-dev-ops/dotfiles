{
  "manifest_version": 2,
  "version": "1.0.1",
  "name": "Automate Browser",
  "description": "Save your devtoos automations to save time.",
  "permissions": [
    "declarativeContent",
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_title": "auto-m8",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
